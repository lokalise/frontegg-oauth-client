        "jsdom": "^28.1.0",
        "msw": "^2.12.10",
        "semantic-release": "^25.0.3",
        "typescript": "^5.9.3",
        "vite": "^6.3.5",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=20"
