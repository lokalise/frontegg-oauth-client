        "jsdom": "^25.0.1",
        "msw": "^2.6.0",
        "semantic-release": "^24.2.0",
        "typescript": "^5.6.3",
        "vite": "^5.4.10",
        "vitest": "^2.1.3"
    },
    "engines": {
        "node": ">=20"
